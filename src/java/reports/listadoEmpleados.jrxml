	<queryString>
		<![CDATA[SELECT DISTINCT
  "empleado"."legajo" as "Legajo",
  "tipoempleado"."nombre" AS "EmpleadoTipo",
  "persona"."apellido" as "Apellido",
  "persona"."nombre" as "Nombre",
  "empleado"."fechaingreso" as "FechaIngreso",
  "empleado"."eliminado" as "Eliminado"
FROM
  public."persona"
  inner join public."empleado" on "empleado"."idempleado" = "persona".id
  inner join public."tipoempleado" on "empleado"."idtipoempleado" = "tipoempleado".id
WHERE
  "empleado"."eliminado" = false or "empleado"."eliminado" is null
ORDER BY
  "persona"."apellido" ASC,
  "persona"."nombre" ASC;]]>
