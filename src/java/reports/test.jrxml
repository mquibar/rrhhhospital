	<queryString>
		<![CDATA[SELECT
     categoria."nombre" AS categoria,
     clase."nombre" AS clase,
     clasevigente."fechavigencia" AS vigencia,
     persona."apellido"||', '||persona."nombre" AS nombre,
     empleado."legajo" AS legajo,
     agrupamiento."nombre" AS agrupamiento,
     tramo."nombre" AS tramo
FROM
     "public"."categoria" categoria INNER JOIN "public"."clasevigente" clasevigente ON categoria."id" = clasevigente."idcategoria"
     INNER JOIN "public"."clase" clase ON clasevigente."idclase" = clase."id"
     INNER JOIN "public"."empleado" empleado ON clasevigente."idempleado" = empleado."idempleado"
     INNER JOIN "public"."persona" persona ON empleado."idempleado" = persona."id"
     INNER JOIN "public"."tramo" tramo ON categoria."idtramo" = tramo."id"
     INNER JOIN "public"."agrupamiento" agrupamiento ON tramo."idagrupamiento" = agrupamiento."id"
WHERE
     persona."id" = 1
ORDER BY
     clasevigente."fechavigencia" ASC]]>
